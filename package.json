    "cross-env": "^7.0.3",
    "eslint": "^9.26.0",
    "globals": "^16.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "3.5.3",
    "supertest": "^7.1.1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.850.0",
    "aws-jwt-verify": "^5.1.0",
